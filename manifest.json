{
  "manifest_version": 2,
  "name": "Chrome Virtual Camera",
  "short_name": "Virtial Cam",
  "version": "1.1",
  "description": "hook getUserMedia() / getUserMedia() を差し替え、時計や動画ファイル、tfjs/body-pixを利用した背景マスクが可能",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://localhost/*",
        "http://127.0.0.1/*",
        "http://localhost:8080/*",
        "http://127.0.0.1:8080/*",
        "http://localhost:5500/*",
        "http://127.0.0.1:5500/*",
        "https://mganeko.github.io/*",
        "https://meet.google.com/*"
      ],
      "js": [
        "loader.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "http://localhost/",
    "http://127.0.0.1/",
    "http://localhost:8080/",
    "http://127.0.0.1:8080/",
    "http://localhost:5500/",
    "http://127.0.0.1:5500/",
    "https://mganeko.github.io/",
    "https://meet.google.com/"
  ],
  "web_accessible_resources": [
    "cs.js"
  ]
}